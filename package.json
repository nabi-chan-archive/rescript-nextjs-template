{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "res:dev": "rescript build -w",
    "res:build": "rescript build",
    "res:clean": "rescript clean",
    "next:dev": "next dev",
    "next:build": "next build",
    "next:start": "next start",
    "next:lint": "next lint",
    "css:dev": "tailwindcss -i src/styles/tailwind.css -o src/styles/dist.css -w",
    "css:build": "tailwindcss -m -i src/styles/globals.css -o src/styles/dist.css",
    "dev": "concurrently \"npm run css:dev\" \"npm run next:dev\""
  },
  "dependencies": {
    "@greenlabs/rescript-next": "^0.3.1",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "next": "13.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@rescript/react": "^0.10.3",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "postcss": "^8.4.19",
    "rescript": "^10.0.1",
    "tailwindcss": "^3.2.4"
  }
}
